{
  "name": "pure-react-carousel",
  "version": "1.15.0",
  "description": "A highly impartial suite of React components that can be assembled by the consumer to create a responsive, aria compliant, carousel with almost no limits on DOM structure or CSS styles. If you're tired of fighting some other developer's CSS and DOM structure, this carousel is for you.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "keywords": [
    "react",
    "carousel",
    "aria",
    "responsive",
    "accessibility",
    "accessible"
  ],
  "scripts": {
    "test": "BABEL_ENV=test jest --no-cache",
    "start": "NODE_ENV=development rollup -w -c rollup.config.dev.js",
    "lint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "eslint ./src --fix --ext .js --ext .jsx",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production rollup -c rollup.config.cjs.js && NODE_ENV=production rollup -c rollup.config.es.js",
    "build:commonjs": "NODE_ENV=production rollup -c rollup.config.cjs.js",
    "build:es": "NODE_ENV=production rollup -c rollup.config.es.js",
    "postbuild": "node postbuild.js",
    "build:watch": "watch 'npm run build' src"
  },
  "typings": "typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/express-labs/pure-react-carousel.git"
  },
  "author": "Matthew Toledo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/express-labs/pure-react-carousel"
  },
  "homepage": "https://github.com/express-labs/pure-react-carousel#readme",
  "devDependencies": {
    "acorn": "^6.0.4",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "clone": "^2.1.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-mrb3k-jest": "^1.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-react": "^7.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.10.0",
    "object.omit": "^3.0.0",
    "raf": "^3.4.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-test-renderer": "^16.6.0",
    "redux": "^4.0.1",
    "regenerator-runtime": "^0.10.5",
    "replace-in-file": "^3.4.2",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^0.6.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-watch": "^4.3.1",
    "uglify-es": "^3.3.5",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "react": "15.x || 16.x",
    "react-dom": "15.x || 16.x"
  },
  "dependencies": {
    "deep-freeze": "0.0.1",
    "deepmerge": "^2.2.1",
    "equals": "^1.0.5",
    "prop-types": "^15.6.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dev/",
      "<rootDir>/dist/"
    ],
    "testMatch": [
      "**/__tests__/**.test.js?(x)"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.jsx"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/",
      "/dist/",
      "/src/App/examples/"
    ],
    "coverageReporters": [
      "text",
      "json",
      "json-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}
