{
  "name": "pure-react-carousel",
  "version": "1.2.0",
  "description": "A highly impartial suite of React components that can be assembled by the consumer to create a carousel with almost no limits on DOM structure or CSS styles. If you're tired of fighting some other developer's CSS and DOM structure, this carousel is for you.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "BABEL_ENV=test jest --no-cache",
    "dev": "NODE_ENV=development rollup -w -c rollup.config.dev.js",
    "build": "NODE_ENV=production rollup -c rollup.config.cjs.js && NODE_ENV=production rollup -c rollup.config.es.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrbinky3000/react-carousel.git"
  },
  "author": "Matthew Toledo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrbinky3000/react-carousel/issues"
  },
  "homepage": "https://github.com/mrbinky3000/react-carousel#readme",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "clone": "^2.1.1",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-config-mrb3k-jest": "^0.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-react": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "object.omit": "^2.0.1",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-modules": "^0.8.0",
    "postcss-simple-vars": "^4.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "regenerator-runtime": "^0.10.5",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-livereload": "^0.4.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-serve": "^0.3.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^3.2.2",
    "uglify-es": "^3.0.15"
  },
  "peerDependencies": {
    "react": "15.x",
    "react-dom": "15.x"
  },
  "dependencies": {
    "deep-freeze": "0.0.1",
    "deepmerge": "^1.3.2",
    "equals": "^1.0.5",
    "object.omit": "^2.0.1",
    "prop-types": "15.x"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/",
      "/dist/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.jsx"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/",
      "/dist/"
    ],
    "coverageReporters": [
      "text",
      "json",
      "json-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  }
}
